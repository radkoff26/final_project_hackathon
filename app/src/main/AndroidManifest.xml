<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mostimportant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MostImportant">
        <activity android:name=".Sport" />
        <activity android:name=".Meditation" />
        <activity android:name=".Eating" />
        <activity android:name=".Walking" />
        <activity android:name=".Rest" />
        <activity android:name=".Activities" />
        <activity android:name=".ResetData" />
        <activity android:name=".input_data" />
        <activity android:name=".Settings" />
        <activity android:name=".input_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="com.example.mostimportant.MAIN_ACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>